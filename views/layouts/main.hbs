        <div class='aside__link'>
          <a href='/john-doe/playlists'>
            <i class="fas fa-compact-disc"></i>
            <span>Library</span>
          </a>
        </div>
      </aside>
      <!-- Music Player -->
      <div class="player__box">
        <div class="player__container">
          <div class="player__info">
            <figure>
              <img src="https://images.pexels.com/photos/6195081/pexels-photo-6195081.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260" alt="Trulli">
              <figcaption>Down, down, down... <p class='author'><small>The awesome band</small></p></figcaption>
            </figure>
          </div>
          <div class="player__controll">
            <div class="player__buttons">
              <button class="btn_player--small"><i class="fas fa-step-backward"></i></button>
              <button class="btn_player--big"><i class="fas fa-pause"></i></button>
              <button class="btn_player--small"><i class="fas fa-step-forward"></i></button>
            </div>
            <div class="player__bar">
              <span class="player__bar--fill"></span>
            </div>
          </div>
          <div class="player__volume">
            <i class="fas fa-volume-up"></i>
            <input class='player__volume--slider' type="range" id="volume" name="volume" min="0" max="10">
          </div>
        </div>
      </div>
    </div>
